{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-njl-final-clean"
		},
		"DataLake1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLake1'"
		},
		"DataLake1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagenjl.dfs.core.windows.net"
		},
		"TR_EVENT_ADLS_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/126c93ce-f0a6-493c-8991-cdcbd797366d/resourceGroups/rg-event-trigger/providers/Microsoft.Storage/storageAccounts/adlseventtriggernjl"
		},
		"TR_EVENT_BLOB_PE_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/126c93ce-f0a6-493c-8991-cdcbd797366d/resourceGroups/rg-adf-trigger2-pe/providers/Microsoft.Storage/storageAccounts/adlseventtrigger2penjl"
		},
		"TR_CUSTOM_EVENT_TEST_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/126c93ce-f0a6-493c-8991-cdcbd797366d/resourceGroups/rg-custom-grid/providers/Microsoft.EventGrid/topics/event-grid-custom-test-1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-njl-final-clean_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLake1_accountKey": {
						"value": "[parameters('DataLake1_accountKey')]"
					},
					"DataLake1_properties_typeProperties_url": {
						"value": "[parameters('DataLake1_properties_typeProperties_url')]"
					},
					"TR_CUSTOM_EVENT_TEST_properties_typeProperties_scope": {
						"value": "[parameters('TR_CUSTOM_EVENT_TEST_properties_typeProperties_scope')]"
					},
					"TR_EVENT_ADLS_properties_typeProperties_scope": {
						"value": "[parameters('TR_EVENT_ADLS_properties_typeProperties_scope')]"
					},
					"TR_EVENT_BLOB_PE_properties_typeProperties_scope": {
						"value": "[parameters('TR_EVENT_BLOB_PE_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}